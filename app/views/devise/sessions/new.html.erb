<body class="sessionsregister_body">


  <div id="banner2">Logging in</div>

<div class="login_registrations">

  <div class="panel-body">



    <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
      <div class="form-group">

        <%= f.email_field :email, class: "form-control", placeholder:"Email", :autofocus => true %>
      </div>
      <div class="form-group">

        <%= f.password_field :password, class: "form-control", placeholder:"Password"%>
      </div>

	<div class="form-group">
        <%= f.submit "Login", class: "btn btn-primary btn_wide" %>
      </div>
    <% end %>
  </div>
  <div id="forgot_pass"><%= render "devise/shared/links" %></div>
 </div>
</div>
<br>
<br>
<br>
<br>



</body>
